{
  "name": "axon-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "docker:up": "docker compose -f supabase-docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f supabase-docker/docker-compose.yml down",
    "docker:logs": "docker compose -f supabase-docker/docker-compose.yml logs -f",
    "db:psql": "docker exec -it supabase-db psql -U postgres",
    "db:push": "cross-env PGSSLMODE=disable supabase db push --db-url postgresql://postgres:jysuXvKLDhmYuyKZSLYQxSzU@127.0.0.1:5433/postgres --yes",
    "db:reset": "docker exec supabase-db psql -U postgres -c \"DROP SCHEMA IF EXISTS supabase_migrations CASCADE; DROP SCHEMA public CASCADE; CREATE SCHEMA public; GRANT ALL ON SCHEMA public TO postgres; GRANT ALL ON SCHEMA public TO public;\" && bun run db:push -- --include-seed",
    "db:diff": "cross-env PGSSLMODE=disable supabase db diff --db-url postgresql://postgres:jysuXvKLDhmYuyKZSLYQxSzU@127.0.0.1:5433/postgres",
    "db:seed": "docker exec -i supabase-db psql -U postgres < supabase/seed.sql"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.15",
    "@ai-sdk/openai": "^3.0.12",
    "@ai-sdk/openai-compatible": "^2.0.13",
    "@anthropic-ai/sdk": "^0.71.2",
    "@monaco-editor/react": "^4.7.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.90.1",
    "ai": "^6.0.39",
    "bcryptjs": "^3.0.3",
    "next": "16.1.2",
    "openai": "^6.16.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20.19.30",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "ignoreScripts": [
    "sharp",
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "sharp",
    "unrs-resolver"
  ]
}
